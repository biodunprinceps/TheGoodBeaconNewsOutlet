{
  "rewrites": [{
    "source": "/(.*)",
    "destination": "/"
  }],
  "headers": [{
      "source": "/styles.css",
      "headers": [{
        "key": "Content-Type",
        "value": "text/css"
      }]
    },
    {
      "source": "/app.js",
      "headers": [{
        "key": "Content-Type",
        "value": "application/javascript"
      }]
    }
  ]
}